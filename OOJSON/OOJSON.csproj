<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>RocketPrinter.$(AssemblyName)</PackageId>
    <PackageProjectUrl>https://github.com/RocketPrinter/OOJSON</PackageProjectUrl>
    <RepositoryUrl>https://github.com/RocketPrinter/OOJSON</RepositoryUrl>
    <Authors>RocketPrinter</Authors>
    <RepositoryType>git</RepositoryType>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>C:\Users\RocketPrinter\source\repos\OOJSON\LICENSE</PackageLicenseFile>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Description>Object oriented JSON</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
